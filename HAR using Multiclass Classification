    "# Import libraries\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.metrics import classification_report, confusion_matrix, accuracy_score\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "fa376a0c-d2e1-4ce0-9a5a-7f414116f365",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load dataset\n",
    "def load_data():\n",
    "    features = pd.read_csv('UCI HAR Dataset/features.txt', delim_whitespace=True, header=None, names=['index', 'feature'])\n",
    "    X_train = pd.read_csv('UCI HAR Dataset/train/X_train.txt', delim_whitespace=True, header=None)\n",
    "    y_train = pd.read_csv('UCI HAR Dataset/train/y_train.txt', header=None, names=['activity'])\n",
    "    X_test = pd.read_csv('UCI HAR Dataset/test/X_test.txt', delim_whitespace=True, header=None)\n",
    "    y_test = pd.read_csv('UCI HAR Dataset/test/y_test.txt', header=None, names=['activity'])\n",
    "    activity_labels = pd.read_csv('UCI HAR Dataset/activity_labels.txt', delim_whitespace=True, header=None, names=['id', 'activity'])\n",
    "    return features, X_train, y_train, X_test, y_test, activity_labels"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "9d7d0260-3d31-4b2c-a912-ad0b302066fa",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Preprocess data\n",
    "def preprocess_data(X_train, X_test, y_train, y_test, activity_labels):\n",
    "    activity_map = dict(activity_labels.values)\n",
    "    y_train['activity'] = y_train['activity'].map(activity_map)\n",
    "    y_test['activity'] = y_test['activity'].map(activity_map)\n",
    "    return X_train, X_test, y_train, y_test"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "40a34bb3-d32a-4f25-b04c-032a867eb971",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Train Random Forest model\n",
    "def train_model(X_train, y_train):\n",
    "    model = RandomForestClassifier(n_estimators=100, random_state=42)\n",
    "    model.fit(X_train, y_train.values.ravel())\n",
    "    return model"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "443716c8-723f-4289-826c-043a64a97b4f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Evaluate the model\n",
    "def evaluate_model(model, X_test, y_test):\n",
    "    y_pred = model.predict(X_test)\n",
    "    acc = accuracy_score(y_test, y_pred)\n",
    "    print(\"Accuracy:\", acc)\n",
    "    print(\"\\nClassification Report:\\n\", classification_report(y_test, y_pred))\n",
    "    cm = confusion_matrix(y_test, y_pred)\n",
    "    sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=np.unique(y_test), yticklabels=np.unique(y_test))\n",
    "    plt.ylabel('Actual')\n",
    "    plt.xlabel('Predicted')\n",
    "    plt.title('Confusion Matrix')\n",
    "    plt.show()"
   ]
  },
